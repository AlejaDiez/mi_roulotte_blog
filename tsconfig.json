{
    "extends": "astro/tsconfigs/strict",
    "include": [".astro/types.d.ts", "**/*"],
    "exclude": ["node_modules", "build"],
    "compilerOptions": {
        "types": ["node"],
        "paths": {
            "@components/*": ["./src/components/*"],
            "@db/*": ["./src/db/*"],
            "@models/*": ["./src/models/*"],
            "@styles/*": ["./src/styles/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "preact"
    }
}
